{
    "name": "vue-boilerplate",
    "version": "0.3.2",
    "private": true,
    "scripts": {
        "start": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "lint": "vue-cli-service lint",
        "lint:css": "stylelint 'src/**/*.vue' --fix",
        "test:unit": "vue-cli-service test:unit",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook"
    },
    "dependencies": {
        "api-manage": "^0.4.0",
        "core-js": "^2.6.5",
        "css.preset": "^0.0.1",
        "element-ui": "^2.7.0",
        "file-saver": "^2.0.1",
        "js-cookie": "^2.2.0",
        "jszip": "^3.2.1",
        "lodash": "^4.17.11",
        "moment": "^2.24.0",
        "normalize.css": "^8.0.1",
        "nprogress": "^0.2.0",
        "vue": "^2.6.6",
        "vue-echarts": "^4.0.1",
        "vue-i18n": "^8.10.0",
        "vue-router": "^3.0.2",
        "vue-rx": "^6.1.0",
        "vuex": "^3.1.0",
        "vuex-persistedstate": "^2.5.4"
    },
    "devDependencies": {
        "@babel/core": "^7.4.5",
        "@storybook/addon-a11y": "^5.0.11",
        "@storybook/addon-actions": "^5.0.11",
        "@storybook/addon-console": "^1.1.0",
        "@storybook/addon-info": "^5.0.11",
        "@storybook/addon-knobs": "^5.0.11",
        "@storybook/addon-links": "^5.0.11",
        "@storybook/addon-storysource": "^5.0.11",
        "@storybook/addon-viewport": "^5.0.11",
        "@storybook/addons": "^5.0.11",
        "@storybook/theming": "^5.0.11",
        "@storybook/vue": "^5.0.11",
        "@vue/cli-plugin-babel": "^3.5.0",
        "@vue/cli-plugin-eslint": "^3.5.0",
        "@vue/cli-plugin-unit-jest": "^3.5.3",
        "@vue/cli-service": "^3.5.0",
        "@vue/test-utils": "1.0.0-beta.29",
        "babel-core": "7.0.0-bridge.0",
        "babel-eslint": "^10.0.1",
        "babel-jest": "^23.6.0",
        "babel-loader": "^8.0.6",
        "babel-plugin-component": "^1.1.1",
        "babel-preset-vue": "^2.0.2",
        "data-mock": "^1.3.0",
        "eslint": "^5.8.0",
        "eslint-plugin-vue": "^5.0.0",
        "less": "^3.9.0",
        "less-loader": "^4.1.0",
        "node-sass": "^4.11.0",
        "sass-loader": "^7.1.0",
        "storybook-addon-vue-info": "^1.2.0",
        "storybook-readme": "^5.0.3",
        "stylelint": "^9.10.1",
        "stylelint-config-recess-order": "^2.0.1",
        "stylelint-config-standard": "^18.2.0",
        "vue-loader": "^15.7.0",
        "vue-template-compiler": "^2.5.21"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true,
            "browser": true
        },
        "extends": [
            "plugin:vue/essential",
            "eslint:recommended"
        ],
        "rules": {
            "indent": [
                0,
                4
            ],
            "quotes": [
                2,
                "single"
            ],
            "linebreak-style": [
                2,
                "unix"
            ],
            "semi": [
                2,
                "never"
            ],
            "no-cond-assign": 2,
            "no-constant-condition": 2,
            "no-dupe-args": 2,
            "no-dupe-keys": 2,
            "no-duplicate-case": 2,
            "no-console": 1,
            "no-debugger": 1,
            "no-var": 1,
            "no-trailing-spaces": 0,
            "eol-last": 0,
            "no-unused-vars": 2,
            "no-unused-expressions": 0,
            "no-underscore-dangle": 0,
            "no-alert": 2,
            "no-lone-blocks": 0,
            "jsx-quotes": 0,
            "vue/script-indent": [
                2,
                4
            ],
            "vue/html-indent": [
                2,
                4
            ],
            "vue/html-self-closing": 2,
            "vue/mustache-interpolation-spacing": [
                "error",
                "always"
            ],
            "vue/no-multi-spaces": [
                "error",
                {
                    "ignoreProperties": false
                }
            ],
            "vue/no-spaces-around-equal-signs-in-attribute": [
                "error"
            ],
            "vue/no-template-shadow": 2,
            "vue/no-restricted-syntax": 2,
            "vue/require-direct-export": 2,
            "vue/no-dupe-keys": 2,
            "vue/no-duplicate-attributes": 2,
            "vue/no-reserved-keys": 2,
            "vue/no-shared-component-data": 2,
            "vue/no-textarea-mustache": 2,
            "vue/no-unused-vars": 2,
            "vue/return-in-computed-property": [
                2,
                {
                    "treatUndefinedAsUnspecified": true
                }
            ],
            "vue/use-v-on-exact": 2,
            "vue/html-end-tags": 2,
            "vue/singleline-html-element-content-newline": [
                "error",
                {
                    "ignoreWhenNoAttributes": true,
                    "ignoreWhenEmpty": true,
                    "ignores": [
                        "pre",
                        "textarea"
                    ]
                }
            ],
            "vue/multiline-html-element-content-newline": [
                "error",
                {
                    "ignoreWhenEmpty": true,
                    "ignores": [
                        "pre",
                        "textarea"
                    ],
                    "allowEmptyLines": false
                }
            ],
            "vue/attributes-order": [
                "error",
                {
                    "order": [
                        "DEFINITION",
                        "LIST_RENDERING",
                        "CONDITIONALS",
                        "RENDER_MODIFIERS",
                        "GLOBAL",
                        "UNIQUE",
                        "TWO_WAY_BINDING",
                        "OTHER_DIRECTIVES",
                        "OTHER_ATTR",
                        "EVENTS",
                        "CONTENT"
                    ]
                }
            ]
        },
        "parserOptions": {
            "parser": "babel-eslint"
        }
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ]
}
